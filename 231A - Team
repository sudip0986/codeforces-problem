#include <stdio.h>
int main()
{
    int p;
    scanf("%d",&p);
    int ns=0;
    for(int i=0; i<p; i++)
    {   
        int n=0;
        int arr[3];
        for(int i=0; i<3; i++)
        scanf("%d",&arr[i]);

        for(int i=0; i<3; i++)
        if(arr[i]==1)
        {
            n++;
        }
        if(n>=2)
        ns++;
    }
    printf("%d",ns);
    return 0;
}
